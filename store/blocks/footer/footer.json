{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": ["flex-layout.row#footer-3-desktop"]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": ["vtex.menu@2.x:menu#Products"],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },

  "flex-layout.row#footer-3-desktop": {
    "children": ["rich-text#footer"],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "Developed by group 17 - by Avanti",
      "blockClass": "footer"
    }
  },

  "footer-layout.mobile": {
    "children": ["flex-layout.row#1-footer-mobile"]
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": ["vtex.menu@2.x:menu#footer-mobile"]
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "Developed by group 17 - by Avanti",
      "blockClass": "footer"
    }
  }
}
